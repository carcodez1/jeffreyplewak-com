{
  "name": "jeffreyplewak-com",
  "version": "0.1.1",
  "private": true,
  "engines": {
    "node": ">=20.19.0 <21"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "CI=1 vitest run",
    "check:lint": "npm run lint",
    "check:test": "npm run test:ci",
    "check:types": "npm run typecheck && npm run typecheck:tests",
    "typecheck:tests": "tsc --noEmit -p tsconfig.tests.json",
    "check:build": "next build",
    "check": "npm run check:lint && npm run check:types && npm run check:test && npm run check:build",
    "diagrams:render": "node scripts/render-diagrams.mjs",
    "diagrams:clean": "node -e \"import('node:fs').then(fs=>fs.rmSync('public/diagrams',{recursive:true,force:true}))\"",
    "diagrams:check": "npm run -s diagrams:render && git diff --exit-code -- public/diagrams",
    "audit:report": "npm audit --audit-level=moderate",
    "yaml:syntax": "node -e \"import('node:fs').then(fs=>import('yaml').then(y=>{y.parse(fs.readFileSync('.github/workflows/diagrams.yml','utf8')); console.log('YAML_SYNTAX_OK');}))\"",
    "prepush": "npm run check && npm run diagrams:check"
  },
  "dependencies": {
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "11.6.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/cheerio": "^0.22.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "cheerio": "^1.2.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vite": "^6.4.1",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0",
    "yaml": "2.8.2"
  }
}
